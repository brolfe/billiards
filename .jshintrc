{
    "globals": {
        "define": false,
        "require": false
    },
    "browser": true,

    "multistr": true,
    "eqnull": true,

    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxdepth": 3,
    "maxstatements": 14,
    "maxcomplexity": 10,
    "maxlen": 120,
    "eqeqeq": true
}
